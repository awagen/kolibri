{
  "type": "AGGREGATE_FROM_DIR_BY_REGEX",
  "weightProvider": {
    "type": "FROM_PER_QUERY_FILE",
    "filePath": "/app/test-files/test-weights/query-weights.txt",
    "removePrefix": "(q=",
    "removeSuffix": ")",
    "columnDelimiter": "  ",
    "keyColumn": 0,
    "weightColumn": 1,
    "defaultValue": 1.0
  },
  "regex": "[(]q=.+[)]",
  "outputFilename": "(ALL-Weighted)",
  "readSubDir": "testJob",
  "writeSubDir": "testJob"
}